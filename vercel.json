{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/assets/(.*)",
            "dest": "/assets/$1"
        },
        {
            "src": "/(.+\\.(?:css|js|jpg|jpeg|png|gif|ico|json))",
            "dest": "/$1"
        },
        {
            "src": "/projects/?$",
            "dest": "/projects.html"
        },
        {
            "src": "/experience/?$",
            "dest": "/experience.html"
        },
        {
            "src": "/$",
            "dest": "/index.html"
        }
    ]
} 